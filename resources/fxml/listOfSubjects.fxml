<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ba.unsa.etf.rpr.ListOfSubjects">
    <stylesheets>
        <URL value="@/css/registration.css" />
    </stylesheets>
    <children>
        <TableView fx:id="table" prefHeight="356.0" prefWidth="400.0">
            <columns>
                <TableColumn fx:id="idColumn" prefWidth="49.0" text="ID" />
                <TableColumn fx:id="nameColumn" prefWidth="233.0" text="Name" />
                <TableColumn fx:id="semesterColumn" prefWidth="114.0" text="Semester" />
            </columns>
        </TableView>
        <Button fx:id="deleteBtn" layoutX="325.0" layoutY="362.0" mnemonicParsing="false" onAction="#deleteProfessor" prefHeight="33.0" prefWidth="70.0" styleClass="addBtn" text="Delete" />
        <Button fx:id="printBtn" layoutX="239.0" layoutY="362.0" mnemonicParsing="false" onAction="#print" prefHeight="33.0" prefWidth="70.0" styleClass="printBtn" text="Print" />
    </children>

</AnchorPane>
