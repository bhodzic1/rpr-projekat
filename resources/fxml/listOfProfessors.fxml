<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ba.unsa.etf.rpr.ListOfProfessors">
    <stylesheets>
        <URL value="@/css/registration.css" />
    </stylesheets>
   <children>
      <TableView fx:id="table" prefHeight="356.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="149.0" text="Name" />
          <TableColumn fx:id="lastnameColumn" prefWidth="149.0" text="Lastname" />
            <TableColumn fx:id="birthdayColumn" prefWidth="149.0" text="Birthday" />
            <TableColumn fx:id="employmentColumn" prefWidth="149.0" text="Employment day" />
        </columns>
      </TableView>
      <Button fx:id="deleteBtn" layoutX="525.0" layoutY="362.0" mnemonicParsing="false" onAction="#deleteProfessor" prefHeight="33.0" prefWidth="70.0" styleClass="addBtn" text="Delete" />
      <Button fx:id="printBtn" layoutX="436.0" layoutY="362.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="70.0" styleClass="printBtn" onAction="#print" text="Print" />
   </children>
</AnchorPane>
