<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ba.unsa.etf.rpr.ListOfProfessors">
   <children>
      <TableView fx:id="table" prefHeight="367.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="150.0" text="Name" />
          <TableColumn fx:id="lastnameColumn" prefWidth="150.0" text="Lastname" />
            <TableColumn fx:id="birthdayColumn" prefWidth="150.0" text="Birthday" />
            <TableColumn fx:id="employmentColumn" prefWidth="150.0" text="Employment day" />
        </columns>
      </TableView>
      <Button fx:id="deleteBtn" onAction="#deleteProfessor" layoutX="532.0" layoutY="360.0" mnemonicParsing="false" text="Delete" />
   </children>
</AnchorPane>
